{
  "v":"4.0",
  "name":"spring-petclinic",
  "workspace":{
    "defaultEnv":"spring-petclinic",
    "environments":[
      {
        "name":"spring-petclinic",
        "machineConfigs":[
          {
            "limits":{
              "ram":2000
            },
            "name":"ws-machine",
            "type":"docker",
            "source":{
              "content":"FROM codenvy/debian_jdk8\nEXPOSE 9966",
              "type":"dockerfile"
            },
            "dev":true,
            "servers":[

            ],
            "envVariables":{

            },
            "links":[

            ]
          }
        ]
      }
    ],
    "projects":[
      {
        "links":[

        ],
        "name":"interview-playground",
        "attributes":{
          "contribute_to_branch":[
            "master"
          ]
        },
        "type":"blank",
        "source": {
          "location": "https://github.com/minted/interview-playground.git",
          "type": "git",
          "parameters": {}
        },
        "path":"/interview-playground",
        "description":"A minted interview playground.",
        "problems":[

        ],
        "mixins":[
          "pullrequest",
          "git"
        ]
      }
    ],
    "name":"minted-interview",
    "commands":[
      {
        "commandLine":"yarn install",
        "name":"install",
        "type":"custom"
      },
      {
        "commandLine":"yarn start",
        "name":"start",
        "attributes":{
          "previewUrl":"http://${server.port.3000}"
        },
        "type":"custom"
      }
    ],
    "links":[

    ]
  },
  "ide":{
    "onProjectsLoaded":{
      "actions":[
        {
          "properties":{
            "name":"install"
          },
          "id":"runCommand"
        },
        {
          "properties":{
            "name":"start"
          },
          "id":"runCommand"
        }
      ]
    }
  }
}
