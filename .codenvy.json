{
  "v": "4.0",
  "workspace": {
    "name": "minted-interview",
    "environments": [
      {
        "machineConfigs": [
          {
            "name": "interview",
            "limits": {
              "ram": 2048
            },
            "type": "docker",
            "source": {
              "type": "image",
              "location": "eclipse/node"
            },
            "servers": {
              "app": {
                "port": "3000",
                "properties": {},
                "protocol": "http"
              },
              "majestic": {
                "port": "3005",
                "properties": {},
                "protocol": "http"
              }
            }
          }
        ]
      }
    ],
    "defaultEnv": "default",
    "projects": [
      {
        "links": [],
        "name": "interview-playground",
        "attributes": {
          "contribute_to_branch": [
            "master"
          ]
        },
        "type": "blank",
        "source": {
          "location": "https://github.com/minted/interview-playground.git",
          "type": "git",
          "parameters": {}
        },
        "path": "/interview-playground",
        "description": "",
        "problems": [],
        "mixins": [
          "pullrequest"
        ]
      }
    ],
    "commands": [
      {
        "commandLine":"yarn install",
        "name":"install",
        "type":"custom"
      },
      {
        "commandLine": "yarn start",
        "name": "Start",
        "type": "custom",
        "attributes": {
          "previewUrl": "http://${server.port.3000}/"
        }
      }
    ],
    "links": []
  },
  "ide":{
    "onProjectsLoaded":{
      "actions":[
        {
          "properties":{
            "name":"install"
          },
          "id":"runCommand"
        },
        {
          "properties":{
            "name":"start"
          },
          "id":"runCommand"
        }
      ]
    }
  }
}
