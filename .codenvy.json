{
  "v":"4.0",
  "name":"minted-interview",
  "workspace":{
    "defaultEnv":"minted-interview",
    "environments": {
      "default": {
        "machines": {
          "dev-machine": {
            "attributes": {
              "memoryLimitBytes": "2147483648"
            },
            "servers": {
              "app": {
                "port": "3000",
                "properties": {},
                "protocol": "http"
              },
              "majestic": {
                "port": "3005",
                "properties": {},
                "protocol": "http"
              }
            },
            "agents": [
              "org.eclipse.che.ssh",
              "org.eclipse.che.ws-agent",
              "org.eclipse.che.terminal",
              "org.eclipse.che.exec"
            ]
          }
        },
        "recipe": {
          "location": "eclipse/node",
          "type": "dockerimage"
        }
      }
    },
    "projects":[
      {
        "links":[

        ],
        "name":"interview-playground",
        "attributes":{
          "contribute_to_branch":[
            "master"
          ]
        },
        "type":"blank",
        "source": {
          "location": "https://github.com/minted/interview-playground.git",
          "type": "git",
          "parameters": {}
        },
        "path":"/interview-playground",
        "description":"A minted interview playground.",
        "problems":[

        ],
        "mixins":[
          "pullrequest",
          "git"
        ]
      }
    ],
    "name":"minted-interview",
    "commands":[
      {
        "commandLine":"yarn install",
        "name":"install",
        "type":"custom"
      },
      {
        "commandLine":"yarn start",
        "name":"start",
        "attributes":{
          "previewUrl":"http://${server.port.3000}"
        },
        "type":"custom"
      }
    ],
    "links":[

    ]
  },
  "ide":{
    "onProjectsLoaded":{
      "actions":[
        {
          "properties":{
            "name":"install"
          },
          "id":"runCommand"
        },
        {
          "properties":{
            "name":"start"
          },
          "id":"runCommand"
        }
      ]
    }
  }
}
