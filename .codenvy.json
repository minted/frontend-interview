{
  "v": "4.0",
  "name": "minted-interview",
  "workspace": {
    "defaultEnv": "minted-interview",
    "environments": [{
      "name": "minted-interview",
      "machineConfigs": [{
        "limits": {
          "ram": 2000
        },
        "name": "ws-machine",
        "type": "docker",
        "source": {
          "location": "eclipse/node",
          "type": "image"
        },
        "dev": true,
        "servers": [],
        "envVariables": {},
        "links": []
      }]
    }],
    "projects": [{
      "links": [],
      "name": "interview-playground",
      "attributes": {
        "contribute_to_branch": [
          "master"
        ]
      },
      "type": "blank",
      "source": {
        "location": "https://github.com/minted/interview-playground.git",
        "type": "git",
        "parameters": {}
      },
      "path": "/interview-playground",
      "description": "A minted interview playground.",
      "problems": [],
      "mixins": [
        "pullrequest",
        "git"
      ]
    }],
    "name": "minted-interview",
    "commands": [{
        "commandLine": "cd ${current.project.path} && yarn install",
        "name": "install",
        "type": "custom"
      },
      {
        "commandLine": "cd ${current.project.path} &&yarn start",
        "name": "start",
        "attributes": {
          "previewUrl": "http://${server.port.3000}"
        },
        "type": "custom"
      }
    ],
    "links": []
  },
  "ide": {
    "onProjectsLoaded": {
      "actions": [{
          "properties": {
            "name": "install"
          },
          "id": "runCommand"
        },
        {
          "properties": {
            "name": "start"
          },
          "id": "runCommand"
        }
      ]
    }
  }
}
