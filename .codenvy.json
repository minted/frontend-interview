{
  "v": "4.0",
  "name": "interview",
  "workspace": {
    "environments": {
      "minted-interview": {
        "machines": {
          "playground": {
            "attributes": {
              "memoryLimitBytes": "2147483648"
            },
            "servers": {
              "app": {
                "port": "3000",
                "properties": {},
                "protocol": "http"
              },
              "tests": {
                "port": "3005",
                "properties": {},
                "protocol": "http"
              }
            },
            "agents": [
              "org.eclipse.che.exec",
              "org.eclipse.che.terminal",
              "org.eclipse.che.ws-agent",
              "org.eclipse.che.ssh"
            ]
          }
        },
        "recipe": {
          "location": "eclipse/node",
          "type": "dockerimage"
        }
      }
    },
    "defaultEnv": "minted-interview",
    "projects": [{
      "links": [],
      "name": "interview-playground",
      "attributes": {
        "contribute_to_branch": [
          "master"
        ],
        "commands": [
          "{\"commandLine\":\"cd /projects/interview-playground && yarn install && touch .installed && yarn start\", \"name\":\"app\", \"attributes\":{\"previewUrl\":\"http://${server.port.3000}\", \"goal\":\"Run\"}, \"type\":\"custom\"}",
          "{\"commandLine\":\"cd /projects/interview-playground && while [ ! -f .installed ]; do sleep 1; done && yarn test\", \"name\":\"tests\", \"attributes\":{\"goal\":\"Test\"}, \"type\":\"custom\"}"
        ]
      },
      "type": "blank",
      "source": {
        "location": "https://github.com/minted/interview-playground.git",
        "type": "git",
        "parameters": {}
      },
      "path": "/interview-playground",
      "description": "A minted interview playground.",
      "problems": [],
      "mixins": []
    }],
    "name": "minted-interview",
    "commands": [],
    "links": []
  },
  "creator": {
    "name": "alanct-minted",
    "email": "alan.thomas@minted.com",
    "created": 1543949102416,
    "userId": "userrf2ysj1dk98g3tha"
  },
  "ide": {
    "onProjectsLoaded": {
      "actions": [{
        "properties": {
          "name": "tests"
        },
        "id": "runCommand"
      }, {
        "properties": {
          "name": "app"
        },
        "id": "runCommand"
      }]
    },
    "onAppLoaded": {
      "actions": [{
        "properties": {
          "greetingTitle": "Welcome!",
          "greetingContentUrl": "https://minted.github.io/interview-playground/welcome"
        },
        "id": "openWelcomePage"
      }]
    }
  }
}
